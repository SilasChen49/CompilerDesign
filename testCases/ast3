ProgNode -> ClassListNode() FuncDefListNode() StatBlockNode() 
StatBlockNode -> VarDeclNode() ForStatNode(for) 
VarDeclNode -> TypeNode(int) IdNode(array) DimListNode() 
DimListNode -> NumNode(100) 
ForStatNode -> VarDeclNode() VarDeclNode() TypeNode(int) IdNode(i) NumNode(0) StatBlockNode() 
VarDeclNode -> TypeNode(int) IdNode(idx) DimListNode() 
VarDeclNode -> IdNode(Apple) IdNode(apple) DimListNode() 
StatBlockNode -> AssignStatNode(=) GetStatNode(get) 
AssignStatNode -> RelExprNode() AddOpNode(+) 
RelExprNode -> VarNode() RelOpNode(<=) NumNode(100) 
VarNode -> DataMemberNode() 
DataMemberNode -> IdNode(i) IndexListNode() 
AddOpNode -> VarNode() NumNode(1) 
VarNode -> DataMemberNode() DataMemberNode() 
DataMemberNode -> IdNode(i) IndexListNode() 
DataMemberNode -> IdNode(i) IndexListNode() 
GetStatNode -> DataMemberNode() 
DataMemberNode -> IdNode(idx) IndexListNode() 
